{
  "name": "iconizza-tools",
  "type": "module",
  "private": true,
  "packageManager": "pnpm@8.8.0",
  "description": "Collection of functions for cleaning up and parsing SVG for Iconizza project",
  "author": "Dennis Ollhoff",
  "license": "MIT",
  "homepage": "https://github.com/iconizza-design/tools",
  "repository": {
    "type": "git",
    "url": "git://github.com/iconizza-design/tools.git"
  },
  "bugs": "https://github.com/iconizza/tools/issues",
  "scripts": {
    "build": "pnpm -r run build",
    "test": "pnpm -r run test",
    "lint": "pnpm -r run lint",
    "lint:fix": "pnpm -r run lint --fix",
    "build:tools": "pnpm -r --filter \"./packages/tools\" run build",
    "test:tools": "pnpm -r --filter \"./packages/tools\" run test:ci",
    "lint:tools": "pnpm -r --filter \"./packages/tools\" run lint",
    "publish:tools": "pnpm -r --filter \"./packages/tools\" run publish",
    "release:tools": "pnpm -r --filter \"./packages/tools\" run release"
  },
  "devDependencies": {
    "@nyxb/eslint-config": "^2.0.0",
    "@types/node": "^18.18.0",
    "eslint": "^8.55.0"
  }
}
